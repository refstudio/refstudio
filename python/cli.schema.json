{
  "type": "object",
  "required": [
    "ingest",
    "rewrite"
  ],
  "properties": {
    "ingest": {
      "$ref": "#/definitions/IngestResponse"
    },
    "rewrite": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/RewriteChoice"
      }
    }
  },
  "description": "CliCommands(ingest: sidecar.typing.IngestResponse, rewrite: list[sidecar.typing.RewriteChoice])",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "definitions": {
    "IngestResponse": {
      "type": "object",
      "required": [
        "project_name",
        "references"
      ],
      "properties": {
        "project_name": {
          "type": "string"
        },
        "references": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Reference"
          }
        }
      },
      "description": "IngestResponse(project_name: str, references: list[sidecar.typing.Reference])"
    },
    "Reference": {
      "type": "object",
      "required": [
        "source_filename",
        "filename_md5"
      ],
      "properties": {
        "source_filename": {
          "type": "string"
        },
        "filename_md5": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "abstract": {
          "type": "string"
        },
        "contents": {
          "type": "string"
        },
        "authors": {
          "type": "array",
          "items": {},
          "default": []
        },
        "chunks": {
          "type": "array",
          "items": {},
          "default": []
        },
        "metadata": {
          "type": "object",
          "default": {}
        }
      },
      "description": "A reference for an academic paper / PDF"
    },
    "RewriteChoice": {
      "type": "object",
      "required": [
        "index",
        "text"
      ],
      "properties": {
        "index": {
          "type": "integer"
        },
        "text": {
          "type": "string"
        }
      },
      "description": "RewriteChoice(index: int, text: str)"
    }
  }
}