{
  "type": "object",
  "required": [
    "ingest",
    "rewrite",
    "chat"
  ],
  "properties": {
    "ingest": {
      "$ref": "#/definitions/IngestResponse"
    },
    "rewrite": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/RewriteChoice"
      }
    },
    "chat": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/ChatResponseChoice"
      }
    }
  },
  "description": "CliCommands(ingest: sidecar.typing.IngestResponse, rewrite: list[sidecar.typing.RewriteChoice], chat: list[sidecar.typing.ChatResponseChoice])",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "definitions": {
    "IngestResponse": {
      "type": "object",
      "required": [
        "project_name",
        "references"
      ],
      "properties": {
        "project_name": {
          "type": "string"
        },
        "references": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Reference"
          }
        }
      },
      "description": "IngestResponse(project_name: str, references: list[sidecar.typing.Reference])"
    },
    "Reference": {
      "type": "object",
      "required": [
        "source_filename",
        "filename_md5"
      ],
      "properties": {
        "source_filename": {
          "type": "string"
        },
        "filename_md5": {
          "type": "string"
        },
        "title": {
          "type": "string"
        },
        "abstract": {
          "type": "string"
        },
        "contents": {
          "type": "string"
        },
        "authors": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Author"
          },
          "default": []
        },
        "chunks": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Chunk"
          },
          "default": []
        },
        "metadata": {
          "type": "object",
          "default": {}
        }
      },
      "description": "A reference for an academic paper / PDF"
    },
    "Author": {
      "type": "object",
      "required": [
        "full_name",
        "given_name",
        "surname",
        "email"
      ],
      "properties": {
        "full_name": {
          "type": "string"
        },
        "given_name": {
          "type": "string"
        },
        "surname": {
          "type": "string"
        },
        "email": {
          "type": "string"
        }
      },
      "description": "Author(full_name: str, given_name: str, surname: str, email: str)"
    },
    "Chunk": {
      "type": "object",
      "required": [
        "text"
      ],
      "properties": {
        "text": {
          "type": "string"
        },
        "vector": {
          "type": "array",
          "items": {
            "type": "number"
          },
          "default": []
        },
        "metadata": {
          "type": "object",
          "default": {}
        }
      },
      "description": "Chunk(text: str, vector: List[float] = <factory>, metadata: Dict[str, Any] = <factory>)"
    },
    "RewriteChoice": {
      "type": "object",
      "required": [
        "index",
        "text"
      ],
      "properties": {
        "index": {
          "type": "integer"
        },
        "text": {
          "type": "string"
        }
      },
      "description": "RewriteChoice(index: int, text: str)"
    },
    "ChatResponseChoice": {
      "type": "object",
      "required": [
        "index",
        "text"
      ],
      "properties": {
        "index": {
          "type": "integer"
        },
        "text": {
          "type": "string"
        }
      },
      "description": "ChatResponseChoice(index: int, text: str)"
    }
  }
}